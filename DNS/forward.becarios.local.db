;
; BIND data file for local loopback interface
;
$ORIGIN becarios.local.
$TTL    604800

@       IN      SOA     ns1.becarios.local. root.becarios.local. (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
; Commentout below three lines
;@      IN      NS      localhost.
;@      IN      A       127.0.0.1
;@      IN      AAAA    ::1

;Name Server Information

@       IN      NS      ns.becarios.local.
@       IN      NS      ns1.becarios.local.

;IP address of Name Server

ns      IN      A       192.168.20.150
ns1     IN      A       192.168.20.150

;A - Record HostName To Ip Address

@       IN       A      192.168.20.120
mail    IN       A      192.168.20.110
ldap    IN       A      192.168.20.130
db      IN       A      192.168.20.140

;CNAME record

www     IN      CNAME   becarios.local.
ftp     IN      CNAME   ns.becarios.local.

;Mail Exchanger
; MX
@       IN       MX   138   mail.becarios.local.

; SPF
@       IN       TXT  "v=spf1 a mx ~all"
